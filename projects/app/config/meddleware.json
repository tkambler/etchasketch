{
  "bodyParserJson": {
    "enabled": true,
    "priority": 1,
    "module": {
      "name": "body-parser",
      "method": "json",
      "arguments": [
        {
          "limit": "200MB"
        }
      ]
    }
  },
  "morgan": {
    "enabled": true,
    "priority": 2,
    "module": {
      "name": "morgan",
      "arguments": ["combined"]
    }
  },
  "cors": {
    "enabled": true,
    "priority": 3,
    "module": {
      "name": "cors",
      "arguments": []
    }
  },
  "session": {
    "enabled": true,
    "priority": 4,
    "module": {
      "name": "express-session",
      "arguments": [
        {
          "secret": "config:sessionSecret",
          "resave": true,
          "saveUninitialized": false
        }
      ]
    }
  },
  "user": {
    "enabled": true,
    "priority": 5,
    "module": {
      "name": "@talkspace/api-service/middleware/user"
    }
  },
  "route-uuid": {
    "enabled": true,
    "priority": 6,
    "module": {
      "name": "@talkspace/api-service/middleware/route-uuid",
      "arguments": [
        {
          "routes": "config:routes"
        }
      ]
    }
  },
  "routes": {
    "enabled": true,
    "priority": 7,
    "module": {
      "name": "@talkspace/api-service/middleware/route-loader",
      "arguments": [
        {
          "routes": "config:routes"
        }
      ]
    }
  }
}
